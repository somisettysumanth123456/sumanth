#TypeErrorHandling
my_list = ["HI", "Hello", "Super", "Hi"]
indices = [0, 1, "2", 3]
for i in range(len(indices)):
	try:
		print(my_list[indices[i]])
	except TypeError:
		print("TypeError: Check list of indices")
